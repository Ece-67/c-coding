#include <stdio.h>

int main() {
    int a[4] = {2, 3, 1, 8};
    printf("{ }\n"); 
    /*
    for (int i = 0; i < 4; i++)
        printf("{%d}\n", a[i]);
    for (int i = 0; i < 4; i++) {
        for (int j = i + 1; j < 4; j++) {
            printf("{%d %d}\n", a[i], a[j]);} }
            */
    for (int i = 0; i < 4; i++) {
        printf("{%d}\n", a[i]);
        for (int j = i + 1; j < 4; j++) {
            printf("{%d %d}\n", a[i], a[j]);
            for (int k = j + 1; k < 4; k++) {
                printf("{%d %d %d}\n", a[i], a[j], a[k]); }}}
    printf("{%d %d %d %d}\n", a[0], a[1], a[2], a[3]);

    return 0;
